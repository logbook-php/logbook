{
    "name": "logbook/logbook",
    "description": "Laravel DevTools.",
    "version": "0.0.1",
    "type": "library",
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/events": "^11.0|^12.0",
        "illuminate/log": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "react/http": "^1.10",
        "react/socket": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-mockery": "^2.0",
        "phpunit/phpunit": "^11.4"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Logbook\\Logbook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Logbook\\Logbook\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Li Zhineng",
            "email": "im@zhineng.li"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpstan analyse"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Logbook\\Logbook\\LogbookServiceProvider"
            ]
        }
    }
}
